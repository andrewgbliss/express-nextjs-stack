{
  "name": "express-nextjs-stack",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.5.0",
    "npm": ">=6.14.5"
  },
  "scripts": {
    "start": "node -r dotenv/config ./build/index.js",
    "build": "node --max-old-space-size=8192 node_modules/typescript/bin/tsc --project tsconfig.next.json",
    "dev": "ts-node-dev --files -r dotenv/config --project tsconfig.next.json --watch -- index.ts",
    "test": "NODE_ENV=test ts-mocha ./src/**/*.spec.ts",
    "db-utils": "sh ./postgres/scripts/db-utils.sh",
    "sequelize": "cd ./build/db && sequelize",
    "lint-staged": "lint-staged",
    "validate": "npm-run-all test lint-staged",
    "clean": "rm -Rf node_modules package-lock.json && npm i",
    "migrate": "npm run build && npm run sequelize db:migrate",
    "seed": "npm run build && npm run sequelize db:seed:all",
    "heroku-postbuild": "npm run build",
    "echo": "echo DATABASE_URL"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.20",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-react-lite": "^2.0.7",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "next": "^9.4.4",
    "nodemailer": "^6.4.10",
    "nodemailer-mailgun-transport": "^2.0.0",
    "notistack": "^0.8.9",
    "npm-run-all": "^4.1.5",
    "parse-domain": "^3.0.2",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "sequelize": "^6.3.0",
    "sequelize-cli": "6.2.0",
    "socket.io": "^2.3.0",
    "typescript": "^3.9.6",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "chai": "^4.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "1.0.0-pre.51"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}